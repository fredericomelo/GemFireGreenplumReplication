 	<region name="GPTablesRowOffset" refid="PROXY"/>
	<region name="AO_rows" refid="PROXY"/>
	<region name="pending_statements" refid="PROXY"/>
	<region name="error_statements" refid="PROXY"/>
	<region name="internal_transactions" refid="PROXY"/>
    
    
</client-cache>